@page  "/responsables/estacionamientos/cajones/ver/{id}"

<!-- Titulo de la Pagina: Inicio -->
<div class="row">
    <div class="col-12">
        <div class="page-title-box">
            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item">Parkner</li>
                    <li class="breadcrumb-item">Responsables</li>
                    <li class="breadcrumb-item">Estacionamientos</li>
                    <li class="breadcrumb-item">Cajones</li>
                    <li class="breadcrumb-item active">Ver</li>
                </ol>
            </div>
            <h4 class="page-title">Detalles del cajón @Modelo.Nombre</h4>
        </div>
    </div>
</div>
<!-- Titulo de la Pagina: Fin -->

<!-- Carta: Inicio -->
<div class="row">
    <div class="col-xl-12">
        <div class="card">
            <div class="card-body text-center">

                    <h4 class="mb-0">@Modelo.Nombre</h4>
                    <p class="text-muted"><Check Disabled="true" Checked="@Modelo.Disponible" ReadOnly="true">Disponible</Check></p>

                    <div class="form-group mt-2 mb-0 text-center">
                        <div class="row">
                            <div class="col-12 mb-1 mb-sm-1 mb-md-0 mb-lg-0 mb-xl-0">
                                <button class="btn btn-block btn-primary waves-light" type="reset" @onclick="Cancelar"> Regresar </button>
                            </div>
                        </div>
                    </div>

            </div>
        </div>
    </div>
</div>
<!-- Carta: Fin -->
